<h1>Sign in</h1>


<%= form_with url: login_path, scope: :session, local: true do |sessions_form| %>

<%= sessions_form.label :email %>
<%= sessions_form.email_field :email, class: "input" %>

<%= sessions_form.label :password %>
<%= sessions_form.password_field :password, class: "input" %>

<%= sessions_form.submit 'Sign In' %>

<% end %>
